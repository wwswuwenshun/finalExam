function start()
{
var port = 1337;
var express = require('express');
var app = express();

app.use(express.static(__dirname));
var route = require("/home/priyankshah/Desktop/newrepo/myexpress/app/routes/index.server.routes.js");
route.init(app);
app.listen(port);
console.log('Server running at http://localhost:' + port);
}
module.exports = start;

